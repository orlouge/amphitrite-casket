{
  "schemaVersion": 1,
  "id": "amphitritecoffer",
  "version": "${version}",

  "name": "Amphitrite Coffer",
  "description": "Adds a portable container found in Ocean Monuments",
  "authors": [
    "orlouge"
  ],
  "contact": {
    "homepage": "https://github.com/orlouge/amphitrite-coffer",
    "sources": "https://github.com/orlouge/amphitrite-coffer"
  },

  "license": "MIT",
  "icon": "assets/amphitritecoffer/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "io.github.orlouge.amphitritecoffer.AmphitriteCofferMod"
    ],
    "client": [
      "io.github.orlouge.amphitritecoffer.AmphitriteCofferClientMod"
    ],
    "modmenu": [
      "io.github.orlouge.amphitritecoffer.config.AmphitriteCofferModMenuIntegration"
    ],
    "rei_client" : [
      "io.github.orlouge.amphitritecoffer.rei.AmphitriteCofferREIClientPlugin"
    ]
  },
  "mixins": [
    "amphitritecoffer.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14",
    "fabric": "*",
    "minecraft": "1.19",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  }
}
